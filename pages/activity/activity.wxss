.main{
  width: 100vw;
  height: 100vh;
  overflow: hidden;
  background: #000;
}
.starting_up{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  z-index: 10;
}
.starting_up .liquor_bg{
  position: absolute;
  top: 0;
  right: -214%;
  height: 100%;
  width: 314%;
}
.loading_content{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.loading_content .start_btn{
  width: 118rpx;
  height: 42rpx;
  margin: 150rpx auto;
  background: none;
}
.loading_content .getUserInfo{
  position: absolute;
  top: 572rpx;
  width: 400rpx;
  height: 86rpx;
  left: 175rpx;
  opacity: 0;
}
.loading_content .loading_progress{
  width: 400rpx;
  height: 86rpx;
  margin: 200rpx auto;
  position: relative;
  z-index: 10;
}
.loading_content .logo{
  width: 466rpx;
  height: 192rpx;
  background: none;
  margin: 180rpx auto 0;
}
.loading_content .loading{
  width: 100%;
  height: 100%;
  background: none;
}
.loading_content .progress_bar{
  width: 370rpx;
  height: 14rpx;
  position: absolute;
  bottom: 12rpx;
  left: 15rpx;
  overflow: hidden; 
}
.progress_bar .progress1{
  background: none;
  width: 5rpx;
  height: 14rpx;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
}
.progress_bar .progress2{
  float: left;
  width: auto;
  overflow: hidden; 
  height: 14rpx;
  margin-left: 2rpx;
}
.progress_bar .progress2 image{
  background: none;
  width: 10rpx;
}
.progress_bar .progress3{
  float: left;
  background: none;
  width: 2rpx;
  display: block;
  height: 14rpx;
}



.activity_content{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 2; 
}

.start_animation{
  width: 100%;
  height: 100%;
  position: relative;
  z-index: 10;
}
.start_animation .animation_img{
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
}
.start_animation .active{
  opacity: 1;
}


.weave{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.weave_show{
  z-index: 99;
}
.weave .bg1,
.weave .bg3,
.weave .bg2{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.weave .bg2{
  animation: bg2animation 2s linear infinite alternate;
}
.damage{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100vw;
  height: 150rpx;
}
.damage .jian{
  width: 32rpx;
  height: 54rpx;
  position: absolute;
  top: 20rpx;
  left: 36rpx;
}
.damage_content{
  width: 444rpx;
  height: 46rpx;
  box-sizing: border-box;
  border: 4rpx solid #FFF;
  border-radius: 8rpx;
  position: absolute;
  top: 28rpx;
  left: 92rpx;
}
.damage_content .damage_num{
  position: absolute;
  top: 6rpx;
  left: 8rpx;
  width: 420rpx;
  height: 26rpx;
  overflow: hidden;
}
.damage_content .damage_num .damage_num_bg{
  width: 0%;
  height: 26rpx;
  background: #FFF;
}
.damage .regulation{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 30rpx;
  left: 556rpx;
}
.damage .reset{
  width: 110rpx;
  height: 56rpx;
  position: absolute;
  top: 24rpx;
  right: 24rpx;
}
.damage .damage_text{
  position: absolute;
  top: 96rpx;
  left: 0;
  width: 85%;
  font-size: 24rpx;
  height: 1em;
  line-height: 1em;
  text-align: center;
  color: #FFF;
}
.damage .reset_text{
  position: absolute;
  top: 96rpx;
  right: 24rpx;
  width: 110rpx;
  font-size: 24rpx;
  height: 1em;
  line-height: 1em;
  text-align: center;
  color: #FFF;
}
.weave .btns{
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  width: 100vw;
  height: 94rpx;
  box-sizing: border-box;
  padding: 0 0 44rpx 36rpx;
}
.btns .btn_item{
  position: absolute;
  bottom: 44rpx;
  left: 36rpx;
  height: 54rpx;
}
.btns .battlefield{
  width: 252rpx;
}
.btns .winelist{
  width: 226rpx;
  left: 288rpx;
}
.btns .share{
  width: 218rpx;
  left: 514rpx;
}
.btns button{
  opacity: 0;
}
.weave .blow{
  position: absolute;
  bottom: 316rpx;
  width: 606rpx;
  height: 216rpx;
  left: 72rpx;
  right: 72rpx;
}
.blow image{
  width: 216rpx;
  height: 216rpx;
  position: absolute;
  bottom: 0;
}
.blow .blow_fist{
  left: 0;
}
.blow .blow_foot{
  right: 0;
}
.weave .blow_animation{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 10;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.6);
}
.blow_animation image{
  width: 660rpx;
  height: 516rpx;
  margin: 40% auto 0;
}
.blow_animation view{
  margin: 60rpx auto;
  color: #FFF;
  font-size: 28rpx;
  text-align: center;
}


.weave image{
  background: none;
}
.weave .dialog{
  color: #FFF;
  font-size: 28rpx;
}
.weave .dialog9,
.weave .dialog1{
  position: absolute;
  bottom: 104rpx;
  left: 60rpx;
  width: 628rpx;
  height: 294rpx;
}
.dialog_text{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.dialog_text .subtitle{
  padding: 54rpx 50rpx 30rpx 50rpx;
  line-height: 1.4em;
}
.dialog_text .select_item{
  width: 90%;
  padding-left: 50rpx;
  line-height: 38rpx;
  height: 38rpx;
  padding-bottom: 26rpx;
}
.dialog_text .select_item image{
  width: 40rpx;
  height: 38rpx;
  display: inline-block;
  float: left;
}
.dialog_text .select_item view{
  line-height: 38rpx;
  display: inline-block;
  float: left;
  margin-left: 30rpx;
}
.dialog_text .desc{
  position: absolute;
  bottom: 30rpx;
  font-size: 24rpx;
  font-weight: 200;
  left: 50rpx;
}
.weave .dialog7,
.weave .dialog8,
.weave .dialog2{
  position: absolute;
  top: 468rpx;
  left: 60rpx;
  width: 628rpx;
  height: 356rpx;
}
.dialog7 .dialog_text .subtitle,
.dialog8 .dialog_text .subtitle,
.dialog2 .dialog_text .subtitle{
  padding: 56rpx 50rpx 40rpx;
}
.dialog7 .dialog_text .select_item,
.dialog8 .dialog_text .select_item,
.dialog2 .dialog_text .select_item{
  padding-bottom: 60rpx;
}
.weave .dialog3{
  position: absolute;
  top: 410rpx;
  left: 60rpx;
  width: 628rpx;
  height: 402rpx;
}
.dialog3 .dialog_text .subtitle{
  padding: 72rpx 50rpx 40rpx;
}
.dialog3 .dialog_text .select_item{
  padding-bottom: 54rpx;
}
.dialog3 .share{
  position: absolute;
  top: 0;
  width: 90%;
  height: 50rpx;
  top: 180rpx;
  left: 50rpx;
  opacity: 0;
}
.weave .dialog5,
.weave .dialog4{
  position: absolute;
  top: 468rpx;
  left: 60rpx;
  width: 628rpx;
  height: 274rpx;
}
.dialog4 .dialog_text .subtitle{
  padding: 96rpx 50rpx;
}
.dialog5 .dialog_text .subtitle{
  padding: 0 50rpx;
  line-height: 274rpx;
}
.dialog7 .ambulance{
  position: absolute;
  width: 466rpx;
  height: 238rpx;
  background: none;
  top: -258rpx;
  left: 81rpx;
}
.dialog7 .paddy_wagon{
  position: absolute;
  width: 502rpx;
  height: 164rpx;
  background: none;
  bottom: -212rpx;
  left: 63rpx;
}
.dialog9 .dialog_text .subtitle{
  padding: 54rpx 0 32rpx 50rpx;
}
.dialog9 .dialog_text .select_item{
  padding-bottom: 26rpx;
}



.weave .dialog6,
.reset_dialog{
  position: absolute;
  top: 468rpx;
  left: 60rpx;
  width: 628rpx;
  height: 356rpx;
}
.dialog6 .dialog_text .subtitle,
.reset_dialog .dialog_text .subtitle{
  padding: 70rpx 50rpx 48rpx;
}
.dialog6 .dialog_text .select_item,
.reset_dialog .dialog_text .select_item{
  padding-bottom: 60rpx;
}

.winelist_list,
.user_battlefield{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 10;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.6);
}
.winelist_list .winelist_content,
.user_battlefield .battlefield_content{
  width: 688rpx;
  height: 1076rpx;
  background: #FFF;
  margin: 20rpx 31rpx;
}


@keyframes bg2animation{
  0%{
    opacity: 1;
  }

  30%{
    opacity: 1;
  }
  35%{
    opacity: 0;
  }
  40%{
    opacity: 1;
  }
  70%{
    opacity: 1;
  }
  75%{
    opacity: 0;
  }
  100%{
    opacity: 1;
  }
}



