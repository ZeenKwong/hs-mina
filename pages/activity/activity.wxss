.supernatant_image{
  position: fixed;
  width: 60rpx;
  height: 80rpx;
  right: -20rpx;
  top: 400rpx;
  z-index: 1001;
}
.supernatant_animation{
  animation: supernatant 600ms ease-out infinite alternate;
}
@keyframes supernatant{
  from {
    right: -23rpx;
  }
  to {
    right: -17rpx;
  }
}
.supernatant_image_active{
  position: fixed;
  right: -50rpx;
  top: 400rpx;
  z-index: 1002;
  animation: supernatant 600ms ease-out infinite alternate;
  border-radius: 50%;
  /* height: 160rpx; */
  /* width: 160rpx; */
  height: 0;
  width: 0;
}
.supernatant{
  width: 100vw;
  height: 100vh;
